{
  "name": "nodehelper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app",
    "lint": "eslint . --ext .js",
    "lint-fix": "eslint . --ext .js --fix",
    "mocha-test": "cross-env-shell MOCHA_REPORTER?=spec mocha --timeout 30000 --exit",
    "test-component": "npm run mocha-test --file 'test/component/setup.spec.js' -- \"test/component/*.spec.js\"",
    "test-integration": "npm run mocha-test --file 'test/unit/setup.spec.js' -- \"test/integration/*.spec.js\"",
    "test-all": "npm run test-component",
    "pretest": "shx rm -rf coverage",
    "test": "nyc --check-coverage npm run test-all"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "buffer-image-size": "^0.6.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.5.7",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "oauth": "^0.9.15",
    "puppeteer": "^1.15.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "serve-favicon": "~2.5.0",
    "sharp": "^0.23.1",
    "sitemap": "^1.13.0",
    "slack-node": "^0.1.8",
    "smartcrop": "^2.0.3",
    "smartcrop-sharp": "^2.0.3",
    "underscore": "^1.9.1",
    "url": "^0.11.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env-default": "^5.1.3-1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "shx": "^0.3.2",
    "spec": "^1.0.1",
    "supertest": "^3.4.2"
  }
}
